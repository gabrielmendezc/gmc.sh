{
  "name": "@gmcsh/web",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "postinstall": "is-ci || husky install",
    "dev": "next dev",
    "test:ci": "jest --ci --bail",
    "build": "next build",
    "start": "next start",
    "test": "jest",
    "lint": "eslint --ignore-path .gitignore --ext .js,.ts,.tsx",
    "check-types": "tsc --noEmit",
    "format": "prettier --write",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "test:e2e": "start-test dev http://localhost:3000/api/health cy:open",
    "pretest:e2e": "rm -rf .nyc_output coverage .next || true",
    "test:e2e:run": "start-test dev http://localhost:3000/api/health cy:run",
    "gen-gql-types": "dotenv -e .env.hasura -- graphql-codegen --config codegen.yml",
    "gen-gql-types:watch": "dotenv -e .env.hasura -- graphql-codegen --config codegen.yml --watch gql/**/*.graphql",
    "export-metadata": "node scripts/export-hasura-metadata.js",
    "precommit": "dotenv -e .env.hasura -- lint-staged"
  },
  "dependencies": {
    "@auth0/nextjs-auth0": "^0.16.0",
    "@headlessui/react": "^0.2.0",
    "@hookform/resolvers": "^1.3.2",
    "@tailwindcss/forms": "^0.2.1",
    "@tailwindcss/typography": "^0.3.1",
    "autoprefixer": "^10.2.1",
    "date-fns": "^2.16.1",
    "graphql": "^15.4.0",
    "graphql-request": "^3.4.0",
    "next": "10.0.5",
    "postcss": "^8.2.3",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-hook-form": "^6.14.0",
    "react-query": "^3.5.11",
    "tailwindcss": "^2.0.2",
    "yup": "^0.32.8"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.20.0",
    "@graphql-codegen/typescript": "1.20.0",
    "@graphql-codegen/typescript-operations": "^1.17.13",
    "@graphql-codegen/typescript-react-query": "^1.0.0",
    "@testing-library/cypress": "^7.0.3",
    "@testing-library/dom": "^7.29.2",
    "@testing-library/jest-dom": "^5.11.8",
    "@testing-library/react": "^11.2.3",
    "@testing-library/user-event": "^12.6.0",
    "@types/faker": "^5.1.5",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.20",
    "@types/react": "^17.0.0",
    "@types/yup": "^0.29.11",
    "@typescript-eslint/eslint-plugin": "^4.12.0",
    "@typescript-eslint/parser": "^4.12.0",
    "babel-jest": "^26.6.3",
    "cypress": "^6.2.1",
    "dotenv": "^8.2.0",
    "dotenv-cli": "^4.0.0",
    "eslint": "^7.17.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "faker": "^5.1.0",
    "hasura-cli": "^1.3.3",
    "husky": "^5.0.6",
    "is-ci": "^2.0.0",
    "jest": "^26.6.3",
    "jest-watch-typeahead": "^0.6.1",
    "lint-staged": "^10.5.3",
    "msw": "^0.25.0",
    "pinst": "^2.1.1",
    "prettier": "^2.2.1",
    "start-server-and-test": "^1.11.7",
    "typescript": "^4.1.3",
    "whatwg-fetch": "^3.5.0"
  }
}
